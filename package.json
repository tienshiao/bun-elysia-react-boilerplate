{
  "name": "bun-elysia-react-boilerplate",
  "version": "1.0.50",
  "scripts": {
    "pretest": "bun run scripts/generate-public-manifest.ts",
    "test": "bun test",
    "dev": "bun run scripts/generate-public-manifest.ts && tsr watch & bun run --watch src/index.ts",
    "build:server": "bun run build-single.ts",
    "build:server:all": "bun run build-all.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "check": "prettier --check . && eslint .",
    "fix": "prettier --write . && eslint . --fix",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.1.10",
    "@elysiajs/bearer": "^1.4.3",
    "@elysiajs/eden": "^1.4.5",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "1.4.7",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-router": "^1.159.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.16",
    "elysia-helmet": "^3.0.0",
    "postgres": "^3.4.8",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@tanstack/react-router-devtools": "^1.159.9",
    "@tanstack/router-cli": "^1.159.9",
    "@types/bun": "latest",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "bun-plugin-tailwind": "^0.1.2",
    "drizzle-kit": "^0.31.9",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.56.0"
  },
  "module": "src/index.js"
}
